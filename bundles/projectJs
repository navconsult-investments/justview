function IsValidEmail(n){var t=n.match(/^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/);return t==null?!1:!0}function IsValidNames(n){var t=n.match(/^([A-z][A-Za-z]*\s+[A-Za-z]*)|([A-z][A-Za-z]*)$/);return t==null?!1:!0}function IsValidPhoneNumber(n){var t=n.match(/^[1-9]\d{2}-[1-9]\d{2}-\d{4}$/);return t==null?!1:!0}$(document).ready(function(){function t(t){t.data.d=="n"&&(n=n-120);t.data.d=="p"&&(n=n+120);i.css({"-webkit-transform":"rotateY("+n+"deg)","-moz-transform":"rotateY("+n+"deg)","-o-transform":"rotateY("+n+"deg)",transform:"rotateY("+n+"deg)"})}var i=$(".d3-carousel"),n=0;$(".d3-next").on("click",{d:"n"},t);$(".d3-prev").on("click",{d:"p"},t)})